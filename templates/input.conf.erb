[[inputs.<%= @plugin_type %>]]
<%      unless @options == nil -%>
<%          @options.sort.each do | option, value | -%>
  <%= option -%> = <%= value.inspect %>
<%          end -%>
<%      end -%>
<% if @sections -%>
<% @sections.sort.each do |section, option| -%>
  [<%= section %>]
<%      unless option == nil -%>
<%          option.sort.each do | suboption, value | -%>
    <%= suboption -%> = <%= value.inspect %>
<%          end -%>
<%      end -%>
<%   end -%>
<% end -%>
