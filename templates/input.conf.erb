[[inputs.<%= @plugin_type %>]]
<%      unless @options == nil -%>
<%          @options.sort.each do | option, value | -%>
  <%= option -%> = <% if value.is_a?(String) %>"<%= value %>"<% elsif value.is_a?(Array) %><%= value.inspect %><% else %><%= value %><% end %>
<%          end -%>
<%      end -%>
<% if @sections -%>
<%   @sections.sort.each do |title, section| -%>
<%     if section['section_type'] -%>
  [[inputs.<%= section['section_type'] %>]]
<%     else -%>
  [[inputs.<%= title %>]]
<%     end -%>
<%     unless section['options'] == nil -%>
<%       section['options'].sort.each do | suboption, value | -%>
    <%= suboption -%> = <% if value.is_a?(String) %>"<%= value %>"<% elsif value.is_a?(Array) %><%= value.inspect %><% else %><%= value %><% end %>
<%       end -%>
<%     end -%>
<%   end -%>
<% end %>
<% if @single_section -%>
<%   @single_section.sort.each do |title, section| -%>
<%     if section['section_type'] -%>
  [inputs.<%= section['section_type'] %>]
<%     else -%>
  [inputs.<%= title %>]
<%     end -%>
<%     unless section['options'] == nil -%>
<%       section['options'].sort.each do | suboption, value | -%>
    <%= suboption -%> = <% if value.is_a?(String) %>"<%= value %>"<% elsif value.is_a?(Array) %><%= value.inspect %><% else %><%= value %><% end %>
<%       end -%>
<%     end -%>
<%   end -%>
<% end -%>
